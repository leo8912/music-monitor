api:
  rate_limit:
    burst_size: 10
    requests_per_minute: 60
  timeout: 30
auth:
  access_token_expire_minutes: 30
  algorithm: HS256
  avatar: /uploads/avatars/35fdc915b375421cbb1250f90a7b692c.jpg
  enabled: true
  password: password
  refresh_token_expire_days: 7
  secret_key: HV1i1DPxwNyWn10XD0lkbHiqa7fsIo6cnW3lrIEF5JM
  username: music
database:
  echo: false
  max_overflow: 10
  pool_size: 5
  url: sqlite+aiosqlite:///./music_monitor.db
download:
  max_concurrent_downloads: 3
  quality_preference: 999
  retry_attempts: 3
  sources:
  - netease
  - qqmusic
  - kugou
  - kuwo
  timeout: 30
global:
  check_interval_minutes: 60
  external_url: http://localhost:5173/
  log_level: INFO
  show_log_panel: true
  theme: light
logging:
  backup_count: 5
  file: logs/application.log
  format: '%(asctime)s - %(name)s - %(levelname)s - %(message)s'
  level: INFO
  max_bytes: 10485760
metadata:
  album_priority:
  - plugin
  cover_priority:
  - plugin
  enable_album: true
  enable_cover: true
  enable_lyrics: true
  lyrics_priority:
  - plugin
  - kugou
  - qqmusic
